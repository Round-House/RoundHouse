#Example '.env' file. A '.env' file should be created in /backend, 
#with this file's contents coppied and unique passwords inputted in all blank variables

# docker-compose.yml - service: api
#DATABASE_URL=postgres://user:password@postgres:5432/db
NODE_ENV=development
PORT=3000

# Auth Module
JWT_SECRET=


# docker-compose.yml - service: postgres
PGHOST=postgres
